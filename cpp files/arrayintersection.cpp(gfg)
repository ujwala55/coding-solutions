#include <bits/stdc++.h>
#define FOR(n1) for(int i=0;i<n1;i++) cin>>a[i]
#define f(n2) for(int i=0;i<n2;i++) cin>>b[i]

using namespace std;

int main() {
	// your code goes here
	int a[10],b[10],n1,n2;
	cin>>n1>>n2;
	FOR(n1);
	f(n2);
	int size=n1+n2;
	for(int i=0;i<n1;i++)
	{
		for(int j=0;j<n2;j++)
		{
			if(a[i]==b[j])
			{
			  cout<<b[j]<<" ";
			}
		}
	}
	
	return 0;
}
